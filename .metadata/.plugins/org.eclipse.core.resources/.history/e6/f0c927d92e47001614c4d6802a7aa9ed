package kr.ac.itschool.practice;

import kr.ac.itschool.entities.Member;

public class JavaBeanTestMain {
	public static void  main(String []args){
		JavaBeanDemo obj = new JavaBeanDemo();
		Member data  = obj.memberGet();
		data.setName("lee");
		System.out.println("주소: "+data.getAddr());
		System.out.println("이름: "+data.getName());
		System.out.println("나이: "+data.getAge());
		System.out.println("직업: "+data.getJob());
		System.out.println("직업: "+data.getPhone());
	}
}
